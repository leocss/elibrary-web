                <li class="active">
                    <a href="{{ app.url_generator.generate('books.index') }}">Books</a>
                </li>
                <li>
                    <a href="{{ app.url_generator.generate('printJobs.index') }}">Printing Service</a>
                </li>
                <li>
                    <a href="{{ app.url_generator.generate('exam.index') }}">Electronic Exam</a>
                </li>
                <li>
                    <a href="{{ app.url_generator.generate('billing.index') }}">Billing</a>
                </li>
            </ul>
        </div>
    </div>
    {% block after_header %}{% endblock %}

    {% block before_content %}{% endblock %}
    <div id="main">
        {% block content %} {% endblock %}
    </div>
    {% block after_content %}{% endblock %}
</div>

